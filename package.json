{
	"name": "empire-cli",
	"version": "0.2.0",
	"description": "",
	"main": "index.js",
	"bin": {
		"empire": "./dist/index.js"
	},
	"scripts": {
		"start": "npm run remove:win:dist && npm run copy:win:archive && npm run build",
		"start:dev": "npm run remove:win:dist && npm run copy:win:archive && npm run build:dev",
		"build": "tsc -p .",
		"build:dev": "tsc -p . -w",
		"refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
		"remove:win:dist": "rmdir /q /s dist",
		"remove:unix:dist": "rm -rf ./dist",
		"copy:win:archive": "xcopy /e /i src\\archive dist\\archive",
		"copy:unix:archive": "cp -r ./src/archive ./dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MrMurdock11/empire-cli.git"
	},
	"author": "Azamat Mambetov",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/MrMurdock11/empire-cli/issues"
	},
	"homepage": "https://github.com/MrMurdock11/empire-cli#readme",
	"dependencies": {
		"adm-zip": "^0.4.14",
		"chalk": "^3.0.0",
		"clear": "^0.1.0",
		"commander": "^7.1.0",
		"directory-tree": "^2.2.4",
		"figlet": "^1.3.0",
		"file-loader": "^6.1.1",
		"inquirer": "^7.1.0",
		"inversify": "^5.0.5",
		"jest": "^26.6.3",
		"lodash": "^4.17.20",
		"path": "^0.12.7",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@babel/core": "^7.12.17",
		"@babel/preset-env": "^7.12.17",
		"@babel/preset-typescript": "^7.12.17",
		"@types/adm-zip": "^0.4.33",
		"@types/clear": "^0.1.0",
		"@types/figlet": "^1.2.0",
		"@types/inquirer": "^6.5.0",
		"@types/jest": "^26.0.20",
		"@types/lodash": "^4.14.165",
		"@types/node": "^13.9.2",
		"babel-loader": "^8.2.2",
		"ts-jest": "^26.5.1",
		"ts-node": "^9.0.0",
		"typescript": "^4.1.5"
	}
}
